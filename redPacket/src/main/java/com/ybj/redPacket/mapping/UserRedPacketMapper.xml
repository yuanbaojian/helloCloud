<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ybj.redPacket.dao.UserRedPacketMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ybj.redPacket.model.UserRedPacket">
        <id column="id" property="id" />
        <result column="red_packet_id" property="redPacketId" />
        <result column="user_id" property="userId" />
        <result column="amount" property="amount" />
        <result column="grab_time" property="grabTime" />
        <result column="note" property="note" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, red_packet_id, user_id, amount, grab_time, note
    </sql>

</mapper>
